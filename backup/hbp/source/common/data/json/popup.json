            "buttonsData": [
                {
                    "id": "popup-next-button",
                    "label": "NEXT",
                    "icon": "icon-filled-arrow-right",
                    "isIconOnLeft": false,
                    "bgColor": "#666666"
                }
            ]
        },
        {
            "text": "Incorrect",
            "textColor": "#e32926",
            "pretextIcon": "icon-close",
