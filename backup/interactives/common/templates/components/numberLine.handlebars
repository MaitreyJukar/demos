<div id="{{idPrefix}}{{tabName}}-drag-drop-numberline">
  <div id="{{idPrefix}}{{tabName}}-drag-drop-container" class="drag-drop-containers"></div>


  <div id="{{idPrefix}}{{tabName}}graph-pane" class="graph-pane">

    <div id="{{idPrefix}}{{tabName}}graph-pane-left-arrow-container" class="graph-pane-left-arrow-container">
      <div id="{{idPrefix}}{{tabName}}graph-pane-left-arrow" class="graph-pane-left-arrow"></div>
      <div id="{{idPrefix}}{{tabName}}graph-pane-left-arrow-line" class="graph-pane-left-arrow-line"></div>
    </div>

    <div id="{{idPrefix}}{{tabName}}graph-pane-middle-line" class="graph-pane-middle-line"></div>

   

    <div id="{{idPrefix}}{{tabName}}graph-pane-middle-graph-points" class="graph-pane-middle-graph-points">

      {{#each range}}
      <div id="{{../idPrefix}}{{../tabName}}graph-pane-{{this}}" class="point-container">
        <div id="{{../idPrefix}}{{../tabName}}graph-pane-{{this}}-point" class="point-box">
          <div class="graph-pane-divider-tick"></div>
        </div>
        <div id="{{../idPrefix}}{{../tabName}}graph-pane-{{this}}-number" class="point-number">{{this}}</div>
      </div>
      {{/each}}

    </div>
    <div id="{{idPrefix}}{{tabName}}graph-pane-slider-box" class="{{idPrefix}}{{tabName}}graph-pane-slider-box graph-pane-slider-box">
      <div id="{{idPrefix}}{{tabName}}graph-pane-slider" class="{{idPrefix}}{{tabName}}graph-pane-slider graph-pane-slider"></div>
    </div>
    <div id="{{idPrefix}}{{tabName}}graph-pane-right-arrow-container" class="graph-pane-right-arrow-container">
      <div id="{{idPrefix}}{{tabName}}graph-pane-right-arrow-line" class="graph-pane-right-arrow-line"></div>
      <div id="{{idPrefix}}{{tabName}}graph-pane-right-arrow" class="graph-pane-right-arrow"></div>
    </div>

    <div id="{{idPrefix}}{{tabName}}-hack-div-slot" class="hack-div-slot"></div>
    
  </div>

</div>   